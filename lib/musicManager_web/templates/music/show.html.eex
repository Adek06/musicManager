<h1>Show Music</h1>

<ul>

  <li>
    <strong>Title:</strong>
    <%= @music.title %>
  </li>
  <li>
    <strong>Year:</strong>
    <%= @music.year %>
  </li>
  <li>
    <strong>Artist:</strong>
    <%= @music.artist.name %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.music_path(@conn, :edit, @music) %></span>
<span><%= link "Back", to: Routes.music_path(@conn, :index) %></span>
